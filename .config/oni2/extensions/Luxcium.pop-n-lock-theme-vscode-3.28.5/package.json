  "scripts": {
    "vsce:update": "yarn run --ignore-engines vsce:global; yarn run --ignore-engines vsce:local; vsce -V",
    "vsce:global": "yarn global add --force --ignore-engines vsce",
    "vsce:local": "yarn add -D --force --ignore-scripts --ignore-engines vsce@latest @types/vscode@1.34.0",
    "vsce:pak-pub": "vsce package --yarn && vsce publish --yarn",
    "package": "vsce package --yarn",
    "only:publish": "vsce publish --yarn",
    "vsce:prepublish": "yarn run --ignore-engines vsce:update; yarn run --ignore-engines test",
    "vsce:publish:minor": "yarn run --ignore-engines vsce:prepublish ; yarn version --ignore-engines  --no-git-tag-version --minor && yarn run --ignore-engines vsce:pak-pub && git add .",
    "vsce:publish:patch": "yarn run --ignore-engines vsce:prepublish ; yarn version --ignore-engines  --no-git-tag-version --patch && yarn run --ignore-engines vsce:pak-pub && git add .",
    "vsce:publish": "yarn run --ignore-engines vsce:prepublish && yarn run --ignore-engines vsce:pak-pub && git add .",
    "test": "snyk test"
  },
